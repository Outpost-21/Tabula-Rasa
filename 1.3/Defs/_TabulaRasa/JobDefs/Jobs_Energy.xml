		<casualInterruptible>false</casualInterruptible>
		<suspendable>false</suspendable>
		<modExtensions>
			<li Class="TabulaRasa.DefModExt_ExtraStrings">
				<extraReportString>recharging TargetB with TargetA.</extraReportString>
			</li>
